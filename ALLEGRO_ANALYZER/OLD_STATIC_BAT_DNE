#!/bin/bash
#SBATCH --job-name=gr
#SBATCH -n 36
#SBATCH -t 8:00:00
#SBATCH -p shared,test
#SBATCH --mem-per-cpu=5000
#SBATCH -o job_%j.out
#SBATCH -e job_%j.err

module list

echo "[**VASP BATCH FILE**] VASP batch file starting (O2)..."
mpirun -np $SLURM_NTASKS /n/kaxiras_lab/vasp.5.4.4-centos7/vasp-O2.std
echo "[**VASP BATCH FILE**] VASP batch file complete."
